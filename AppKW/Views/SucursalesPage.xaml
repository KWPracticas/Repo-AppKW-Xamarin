             xmlns:vm="clr-namespace:AppKW.ViewModels"
             x:Class="AppKW.Views.SucursalesPage"
             Title="{Binding Title}">
    
    <ContentPage.BindingContext>
        <vm:SucursalesViewModel/>
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout>
           <!-- <ScrollView> -->
                <StackLayout Padding="0" Spacing="0">
                <StackLayout>
                    <WebView HeightRequest="800" WidthRequest="640" x:Name="url_mapa" />
                </StackLayout>
            </StackLayout>
           <!-- </ScrollView> -->
        </StackLayout>
    </ContentPage.Content>
</ContentPage>